{
    "routes": [
      {
        "route": "/api/search",
        "methods": ["get"],
        "rewrite": "/api/search"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/api/*"]
    },
    "platform": {
      "apiRuntime": "dotnet-isolated"
    },
    "globalHeaders": {
      "Access-Control-Allow-Origin": "*",
      "Cache-Control": "no-store, must-revalidate, no-cache, max-age=0",
      "X-Content-Type-Options": "nosniff"
    },
    "mimeTypes": {
      ".js": "application/javascript",
      ".jsx": "application/javascript"
    }
  }
  